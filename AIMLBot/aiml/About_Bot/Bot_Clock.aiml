  <!-- Phase of a day  check -->

  <category>
    <pattern>DAY PHASE</pattern>
    <template>
      <think>
        <set var="hour">
          <date format="hh" />
        </set>
        <set var="ampm">
          <date format="tt" />
        </set>
      </think>
      <condition>
        <li value="AM" var="ampm">Morning</li>
        <li>
          <condition var="hour">
            <li value="12">Noon</li>
            <li value="01">Afternoon</li>
            <li value="02">Afternoon</li>
            <li value="03">Afternoon</li>
            <li value="04">Afternoon</li>
            <li value="05">Afternoon</li>
            <li value="06">Afternoon</li>
            <li>Night</li>
          </condition>
        </li>
      </condition>
    </template>
  </category>

