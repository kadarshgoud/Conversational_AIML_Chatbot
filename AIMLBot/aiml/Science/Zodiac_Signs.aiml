    <template>I don't really believe in horoscopes.</template>
  </category>

  <category>
    <pattern>ARE YOU A CAPRICORN</pattern>
    <template>
      <think>
        <set name="userzodiacsign">
          <bot name="botzodiacsign"/>
        </set>
      </think>
      <condition name="userzodiacsign">
        <li value ="CAPRICORN">
          Yes, You sign is <bot name="botzodiacsign"/>.
        </li>
        <li value ="unknown"> No idea. You haven't told me your sign.</li>
        <li>
          No, You told me that your sign is <bot name="botzodiacsign"/>.
        </li>
      </condition>
    </template>
  </category>

